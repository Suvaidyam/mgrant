 "font_size": 14,
 "html": "<link href=\"https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css\" rel=\"stylesheet\">\n<h1 class=\"text-2xl font-bold mb-1\">Details </h1>\n<div class=\"bg-white p-6 mb-4 rounded-lg shadow-lg\">\n    <!--<h2 class=\"text-2xl font-bold mb-4\">Project Information</h2>-->\n\n    <div class=\"grid grid-cols-1 sm:grid-cols-2 gap-6\">\n        <div>\n            <p class=\"text-md font-semibold\">NGO:</p>\n            <p class=\"text-gray-700\">{{ doc.ngo }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Donor:</p>\n            <p class=\"text-gray-700\">{{ doc.donor }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Grant Name:</p>\n            <p class=\"text-gray-700\">{{ doc.grant_name }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Grant Type:</p>\n            <p class=\"text-gray-700\">{{ doc.grant_type }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Start Date:</p>\n            <p class=\"text-gray-700\">{{ doc.start_date }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">End Date:</p>\n            <p class=\"text-gray-700\">{{ doc.end_date }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Grant Duration in months:</p>\n            <p class=\"text-gray-700\">{{ doc.grant_duration_in_months }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Funding Type:</p>\n            <p class=\"text-gray-700\">{{ doc.funding_type }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Implementation Category:</p>\n            <p class=\"text-gray-700\">{{ doc.implementation_category }}</p>\n        </div>\n        <div>\n            <p class=\"text-md font-semibold\">Implementation Category:</p>\n            <p class=\"text-gray-700\">{{ doc.subgranting_status }}</p>\n        </div>\n    </div>\n</div>\n\n\n<h1 class=\"text-2xl font-bold mb-2\">Grant Receipts</h1>\n{% set get_tranches = frappe.db.get_list('Grant Receipts', filters={'grant': doc.name }, fields=['name','financial_year','tranch_no','total_funds_planned','planned_due_date','request_status','funds_requested','fund_receipt_status','total_funds_received']) %}\n\n<div class=\"overflow-auto custom-scrollbar\" style=\"max-height: 400px;\">\n    <table class=\"min-w-full table-auto border-collapse border border-gray-300 mt-1\">\n        <thead>\n            <tr class=\"bg-gray-100\">\n                <th class=\"px-4 w-[200px] py-2 border border-gray-300\" style=\"width: 100px;\">ID</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Financial Year</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 100px;\">Tranch No</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Total Funds Planned</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Planned Due Date</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Request Status</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Funds Requested</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Fund Receipt Status</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Total Funds Received</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for tranche in get_tranches %}\n            <tr x-data=\"{ open: false }\" class=\"odd:bg-white even:bg-gray-50\">\n                <td style=\"width: 400px;\" class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.name }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.financial_year }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.tranch_no }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.total_funds_planned }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.planned_due_date }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.request_status }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.funds_requested}}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.fund_receipt_status}}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ tranche.total_funds_received}}</td>\n            </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n</div>\n\n <!--=========================== Budget Table ===============================-->\n \n<h1 class=\"text-2xl font-bold mt-2 mb-3\">Budget Plan and Utilisation Details</h1>\n{% set budget_data = frappe.db.get_list('Budget Plan and Utilisation', filters={'grant': doc.name}, fields=['*'])%}\n<div class=\"overflow-auto custom-scrollbar\" style=\"max-height: 400px; border: 1px solid #ccc;\">\n    <table class=\"min-w-full table-auto border-collapse border border-gray-300 rounded-xl\">\n        <thead>\n            <tr class=\"bg-gray-100\">\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 100px;\">ID</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Budget Head</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Item Name</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 150px;\">Financial Year</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q1 (Apr-Jun) Planned Budget</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q2 (Jul-Sep) Planned Budget</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q3 (Oct-Dec) Planned Budget</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q4 (Jan-Mar) Planned Budget</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Total Planned Budget</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q1 (Apr-Jun) Utilisation</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q2 (Jul-Sep) Utilisation</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q3 (Oct-Dec) Utilisation</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Q4 (Jan-Mar) Utilisation</th>\n                <th class=\"px-4 py-2 border border-gray-300\" style=\"width: 200px;\">Total Utilisation</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for budget in budget_data %}\n            <tr x-data=\"{ open: false }\" class=\"odd:bg-white even:bg-gray-50\">\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.name }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.budget_head }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.item_name }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.financial_year }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q1_apr_jun_planned_budget }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q2_jul_sep_planned_budget }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q3_oct_dec_planned_budget }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q4_jan_mar_planned_budget }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.total_planned_budget }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q1_apr_jun_utilisation }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q2_jul_sep_utilisation }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q3_oct_dec_utilisation }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.q4_jan_mar_utilisation }}</td>\n                <td class=\"px-4 py-2 text-black border border-gray-300\">{{ budget.total_utilisation }}</td>\n            </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n</div>\n",
